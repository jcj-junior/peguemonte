rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // Apenas o proprietário autenticado (você) tem acesso total
      // Substitua 'SEU_UID_DE_ADMIN' pelo seu UID do Firebase Auth após o login
      allow read, write: if request.auth != null; 
    }
  }
}
